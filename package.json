{
  "name": "vscode-gestalt",
  "displayName": "Gestalt",
  "description": "Pinterest's design system extension",
  "publisher": "pinterest",
  "version": "0.0.458",
  "engines": {
    "vscode": "^1.57.0"
  },
  "keywords": [
    "Pinterest",
    "Gestalt",
    "Design System"
  ],
  "license": "Apache-2.0",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onView:gestaltView"
  ],
  "main": "./out/extension.js",
  "icon": "images/vscode-gestalt-256.png",
  "configurationDefaults": {
    "[javascript]": {
      "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
      }
    }
  },
  "contributes": {
    "commands": [
      {
        "command": "gestalt.track.snippet",
        "title": "Track snippet insertions"
      },
      {
        "command": "gestalt.insertSnippetFromTreeView",
        "title": "Insert snippet from tree view"
      },
      {
        "command": "gestalt.openUrl",
        "title": "Open a URL in the browser"
      },
      {
        "command": "gestalt.quickPick",
        "title": "Gestalt: Quick Pick"
      },
      {
        "command": "gestalt.openComponentUrl",
        "title": "Gestalt: Open Component URL",
        "icon": "$(globe)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "gestalt.quickPick",
          "title": "Gestalt: Quick Pick"
        }
      ],
      "view/item/context": [
        {
          "command": "gestalt.openComponentUrl",
          "commandArguments": [
            "${1:component}"
          ],
          "when": "view == gestalt-view-components",
          "group": "inline"
        }
      ]
    },
    "keybindings": [
      {
        "command": "gestalt.quickPick",
        "key": "Alt+G",
        "mac": "Alt+G",
        "when": "editorTextFocus"
      }
    ],
    "configuration": [
      {
        "title": "Gestalt",
        "properties": {
          "gestalt.enableTelemetry": {
            "default": true,
            "description": "Enable usage data and errors tracking (Google Analytics)",
            "scope": "window",
            "type": "boolean"
          }
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gestalt-view",
          "title": "Gestalt",
          "icon": "images/gestalt-logo.svg"
        }
      ]
    },
    "views": {
      "gestalt-view": [
        {
          "id": "gestalt-view-components",
          "name": "Components"
        },
        {
          "id": "gestalt-view-help",
          "name": "Help"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "generate:snippets": "node ./scripts/generateSnippets.mjs",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "jest": "jest"
  },
  "devDependencies": {
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.1",
    "@types/node": "16.11.12",
    "@types/uuid": "^8.3.3",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.5.0",
    "glob": "^7.2.0",
    "jest": "^27.1.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-syntax-flow": "^7.16.5",
    "execa": "^5.1.1",
    "got": "^11.8.3",
    "react-docgen": "^5.4.0",
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pinterest/vscode-gestalt"
  }
}
